{
  "projectType": "library",
  "sourceRoot": "libs/app-conf/src",
  "prefix": "smart-home",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/app-conf"
      ],
      "options": {
        "jestConfig": "libs/app-conf/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/app-conf/**/*.ts",
          "libs/app-conf/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
